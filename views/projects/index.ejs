<%- include('../partials/header') %>

<h1><%= title %></h1>

<table>
  <thead>
    <tr>
      <th>Project Name</th>
      <th>First Appearance</th>
    </tr>
  </thead>
  <tbody>
    <% projects.forEach((p) => { %>
    <tr>
      <td><%= p.name %></td>
      <td><%= p.firstAppear %></td>
    </tr>
    <% }) %>
  </tbody>
</table>

<a href="/projects/new">Add New Project</a>

<%- include('../partials/footer') %>